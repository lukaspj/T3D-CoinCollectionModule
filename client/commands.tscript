function clientCmdShowVictory(%score)
{
    MessageBoxOK("You Win!",
            "Congratulation you found" SPC %score SPC "coins!",
            "disconnect();" );
}

function clientCmdShowDefeat(%score)
{
    MessageBoxOK("You Lost!",
            "You lost with" SPC %score SPC "coins",
            "disconnect();" );
}
